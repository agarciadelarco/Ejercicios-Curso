<%= form_for @concert do |f| %>
<p>
  <%= f.label :band %><br>
  <%= f.text_field :band %>
</p>

<p>
  <%= f.label :venue %><br>
  <%= f.text_field :venue %>
</p>

<p>
  <%= f.label :city %><br>
  <%= f.text_field :city %>
</p>

<p>
  <%= f.label :date %><br>
  <%= f.text_field :date %>
</p>

<p>
  <%= f.label :price %><br>
  <%= f.text_field :price %>
</p>

<p>
  <%= f.label :description %><br>
  <%= f.text_area :description %>
</p>

<p>
  <%= f.submit %>
</p>

<% end %>

<p>
  <%= link_to 'Back', concerts_path %>
</p>